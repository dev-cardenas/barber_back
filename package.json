{
  "name": "barber",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:dev-cardenas/goBarberBack.git",
  "license": "MIT",
  "engines": {
    "node": "18.16.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf ./build",
    "start:prod": "node dist/server.js",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "lint": "eslint src",
    "dev": "ts-node-dev --files -r tsconfig-paths/register src/server.ts",
    "dev:fastify": "tsx watch src/serverPractica.ts",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "yarn typeorm migration:run -d ./src/config/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d ./src/config/data-source.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@sentry/node": "^5.7.0",
    "bcrypt": "^5.1.0",
    "bee-queue": "^1.2.2",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fastify": "^4.17.0",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.7.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.3.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.13",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "youch": "^3.2.3",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.14.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  },
  "author": "dev-cardenas <dev.cardenas@gmail.com>"
}
